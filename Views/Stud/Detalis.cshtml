@model Student

<partial name="_StudentCard" model="@Model" />
<a class="btn btn-success"
   asp-action="Delete"
   asp-controller="Stud"
   asp-route-id="@Model.Id"
   onclick="ConfirmDelete(event)">Delete</a>

<script>
    function ConfirmDelete() {
        document.getElementById("")
        var accept = confirm("Are You Sure?");
        if (accept == false) {
            event.preventDefault();//pervent go to server
        }
    }
</script>
